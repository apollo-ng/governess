{
  "name": "governess-app",
  "description": "Angular2/Ionic2 hybrid user interface for governess",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "homepage": "https://apollo.open-resource.org/lab:governess:app",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollo-ng/governess.git"
  },
  "bugs": {
    "url": "https://github.com/apollo-ng/governess/issues"
  },
  "private": true,
  "scripts": {
    "build": "ionic-app-scripts build",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build",
    "postinstall": "ionic state restore",
    "electron": "electron .",
    "electron dist-linux-x64": "electron-packager . governess-app --app-version=1.0.0 --platform=linux --arch=x64 --prune --asar --overwrite --ignore=config --ignore=platforms --ignore=node_modules --ignore=coverage --ignore=resources --ignore=tests --ignore=plugins --ignore=src --ignore=hooks"
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/compiler-cli": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/platform-server": "2.1.1",
    "@ionic/storage": "1.1.6",
    "angular2-uuid": "1.1.0",
    "ionic-angular": "2.0.0-rc.2",
    "ionic-native": "2.2.3",
    "ionicons": "3.0.0",
    "ng2-charts": "1.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@ionic/app-scripts": "0.0.39",
    "electron": "1.4.4",
    "electron-packager": "8.2.0",
    "typescript": "2.0.6"
  },
  "description": "iontest1: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-vibration",
    "phonegap-plugin-barcodescanner",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "android"
  ],
  "main": "electron.js"
