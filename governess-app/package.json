{
  "name": "governess-app",
  "description": "Angular2/Ionic2 hybrid user interface for governess",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "homepage": "https://apollo.open-resource.org/lab:governess:app",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollo-ng/governess.git"
  },
  "bugs": {
    "url": "https://github.com/apollo-ng/governess/issues"
  },
  "scripts": {
    "build": "gulp --gulpfile tests/gulpfile.ts --cwd ./ build-app",
    "e2e": "gulp --gulpfile tests/gulpfile.ts --cwd ./ build-e2e && protractor tests/protractor.conf.js",
    "karma": "gulp --gulpfile tests/gulpfile.ts --cwd ./ karma-debug",
    "postinstall": "ionic state restore && typings install && webdriver-manager update && cp tests/karma-static/*.html node_modules/karma/static",
    "start": "ionic serve",
    "test": "gulp --gulpfile tests/gulpfile.ts --cwd ./ unit-test",
    "electron": "electron .",
    "electron dist-linux-x64": "electron-packager . governess-app --app-version=1.0.0 --platform=linux --arch=x64 --prune --asar --overwrite --ignore=platforms --ignore=node_modules --ignore=coverage --ignore=resources --ignore=tests --ignore=plugins --ignore=app --ignore=typings --ignore=hooks"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "angular2-uuid": "^1.0.7",
    "es6-shim": "^0.35.0",
    "ionic-angular": "2.0.0-beta.11",
    "ionic-native": "1.3.10",
    "ionicons": "3.0.0",
    "lodash": "^4.15.0",
    "ng2-charts": "1.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "browserify": "13.0.1",
    "browserify-istanbul": "2.0.0",
    "codecov.io": "0.1.6",
    "del": "2.2.1",
    "electron-packager": "7.5.1",
    "electron-prebuilt": "1.3.3",
    "gulp": "3.9.1",
    "gulp-tslint": "5.0.0",
    "gulp-typescript": "2.13.6",
    "gulp-watch": "4.3.8",
    "ionic-gulp-browserify-typescript": "2.0.0",
    "ionic-gulp-fonts-copy": "1.0.0",
    "ionic-gulp-html-copy": "1.0.0",
    "ionic-gulp-sass-build": "1.0.0",
    "ionic-gulp-scripts-copy": "2.0.1",
    "ionic-gulp-tslint": "^1.0.0",
    "isparta": "4.0.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.1.0",
    "karma-browserify": "5.0.5",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "phantomjs-prebuilt": "2.1.7",
    "protractor": "3.3.0",
    "run-sequence": "1.2.1",
    "ts-node": "0.9.3",
    "tsify": "0.16.0",
    "tslint": "3.12.1",
    "tslint-eslint-rules": "1.3.0",
    "tslint-ionic-rules": "^0.0.3",
    "typings": "1.3.1"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-vibration",
    "phonegap-plugin-barcodescanner"
  ],
  "cordovaPlatforms": [
    "android"
  ],
  "main": "electron.js"
}
