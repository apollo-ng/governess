{
  "name": "governess-ui",
  "version": "1.0.0",
  "description": "Angular/Ionic2 hybrid user interface for governess",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollo-ng/governess.git"
  },
  "bugs": {
    "url" : "https://github.com/apollo-ng/governess/issues"
  },
  "scripts": {
    "build": "gulp --gulpfile test/gulpfile.ts --cwd ./ build-app",
    "e2e": "gulp --gulpfile test/gulpfile.ts --cwd ./ build-e2e && protractor test/protractor.conf.js",
    "karma": "gulp --gulpfile test/gulpfile.ts --cwd ./ karma-debug",
    "postinstall": "ionic state restore && typings install && webdriver-manager update && cp test/karma-static/*.html node_modules/karma/static",
    "start": "ionic serve",
    "test": "gulp --gulpfile test/gulpfile.ts --cwd ./ unit-test"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "es6-shim": "0.35.1",
    "ionic-angular": "2.0.0-beta.8",
    "ionic-native": "1.2.2",
    "ionicons": "3.0.0",
    "ng2-translate": "^2.1.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "browserify": "13.0.0",
    "browserify-istanbul": "2.0.0",
    "codecov.io": "0.1.6",
    "del": "2.2.0",
    "gulp": "3.9.1",
    "gulp-tslint": "5.0.0",
    "gulp-typescript": "2.13.4",
    "gulp-watch": "4.3.5",
    "ionic-gulp-browserify-typescript": "^1.1.0",
    "ionic-gulp-fonts-copy": "^1.0.0",
    "ionic-gulp-html-copy": "^1.0.0",
    "ionic-gulp-sass-build": "^1.0.0",
    "ionic-gulp-scripts-copy": "^2.0.0",
    "isparta": "4.0.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.4.0",
    "karma": "0.13.22",
    "karma-browserify": "5.0.5",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.0.3",
    "karma-phantomjs-launcher": "1.0.0",
    "phantomjs-prebuilt": "2.1.7",
    "protractor": "3.3.0",
    "run-sequence": "1.2.0",
    "ts-node": "0.7.3",
    "tsify": "0.16.0",
    "tslint": "3.10.2",
    "tslint-eslint-rules": "^1.3.0",
    "typings": "1.0.4"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "android"
  ]
}
